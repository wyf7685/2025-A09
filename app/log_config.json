{
  "version": 1,
  "disable_existing_loggers": false,
  "handlers": {
    "default": {
      "class": "app.log.LoguruHandler"
    }
  },
  "loggers": {
    "uvicorn.error": { "handlers": ["default"], "level": "INFO" },
    "uvicorn.access": {
      "handlers": ["default"],
      "level": "INFO"
    }
  }
}
